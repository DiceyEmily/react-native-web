{
  "name": "smart_client_mobile",
  "version": "2.0.1",
  "private": true,
  "homepage": ".",
  "scripts": {
    "json_model": "cross-env ts-node --project src/rn-common/taskconfig.json ./src/rn-common/task/generateCode.ts",
    "model代码合并": "ts-node --project src/rn-common/taskconfig.json ./src/rn-common/task/merge.ts",
    "web": "cross-env ENVFILE=.env.smartcampus  PORT=5010 react-app-rewired start",
    "web test": "cross-env ENVFILE=.env.smartcampus.test  PORT=5011 react-app-rewired start",
    "build web": "cross-env ENVFILE=.env.smartcampus react-app-rewired build",
    "build android js": "cross-env ENVPROJECT=smartcampus ts-node --project src/rn-common/taskconfig.json ./src/rn-common/task/packAndroidJs.ts",
    "build android apk": "cd android && cross-env ENVFILE=.env.smartcampus ./gradlew assembleRelease",
    "build android apk test": "cd android && cross-env ENVFILE=.env.smartcampus.test ./gradlew assembleRelease",
    "android": "cross-env ENVFILE=.env.smartcampus react-native run-android",
    "android test": "cross-env ENVFILE=.env.smartcampus.test react-native run-android",
    "rn start": "cross-env ENVFILE=.env.smartcampus.test react-native start",
    "analyze": "source-map-explorer 'build/smartcampus/static/js/*.js'",
    "test": "jest",
    "lint": "eslint .",
    "postinstall": "ts-node --project src/rn-common/taskconfig.json ./src/rn-common/task/fixSource.ts"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
